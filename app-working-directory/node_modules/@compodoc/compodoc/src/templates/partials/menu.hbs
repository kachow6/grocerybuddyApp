                {{/compare}}>
                {{#each classes}}
                    <li class="link">
                        <a href="{{relativeURL ../depth 'classes'}}classes/{{name}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if injectables}}
        <li class="chapter">
            <div class="simple">
                <span class="fa fa-long-arrow-down"></span>
                <span>Injectables</span>
                <span class="menu-toggler fa {{#isNotToggle 'injectables'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#injectables-links"
                    {{else}}
                        data-target="#xs-injectables-links"
                    {{/compare}}></span>
            </div>
            <ul class="links collapse {{#isNotToggle 'injectables'}}in{{/isNotToggle}}"
                {{#compare menu "===" 'normal' }}
                    id="injectables-links"
                {{else}}
                    id="xs-injectables-links"
                {{/compare}}>
                {{#each injectables}}
                    <li class="link">
                        <a href="{{relativeURL ../depth 'injectables'}}injectables/{{name}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if interfaces}}
        <li class="chapter">
            <div class="simple">
                <span class="fa fa-info"></span>
                <span>Interfaces</span>
                <span class="menu-toggler fa {{#isNotToggle 'interfaces'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#interfaces-links"
                    {{else}}
