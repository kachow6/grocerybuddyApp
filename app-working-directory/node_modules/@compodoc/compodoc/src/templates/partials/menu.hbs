            <ul class="links collapse {{#isNotToggle 'components'}}in{{/isNotToggle}}"
                {{#compare menu "===" 'normal' }}
                    id="directives-links"
                {{else}}
                    id="xs-directives-links"
                {{/compare}}>
                {{#each directives}}
                    <li class="link">
                        <a href="{{relativeURL ../depth 'directives'}}directives/{{name}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if classes}}
        <li class="chapter">
            <div class="simple">
                <span class="fa fa-file-code-o"></span>
                <span>Classes</span>
                <span class="menu-toggler fa {{#isNotToggle 'classes'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#classes-links"
                    {{else}}
                        data-target="#xs-classes-links"
