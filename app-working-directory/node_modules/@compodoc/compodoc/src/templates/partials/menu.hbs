                    {{/compare}}></span>
            </div>
            <ul class="links collapse {{#isNotToggle 'interfaces'}}in{{/isNotToggle}}"
                {{#compare menu "===" 'normal' }}
                    id="interfaces-links"
                {{else}}
                    id="xs-interfaces-links"
                {{/compare}}>
                {{#each interfaces}}
                    <li class="link">
                        <a href="{{relativeURL ../depth 'interfaces'}}interfaces/{{name}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if pipes}}
        <li class="chapter">
            <div class="simple">
                <span class="fa fa-plus"></span>
                <span>Pipes</span>
                <span class="menu-toggler fa {{#isNotToggle 'pipes'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#pipes-links"
                    {{else}}
                        data-target="#xs-pipes-links"
                    {{/compare}}></span>
            </div>
            <ul class="links collapse {{#isNotToggle 'pipes'}}in{{/isNotToggle}}"
                {{#compare menu "===" 'normal' }}
                    id="pipes-links"
                {{else}}
                    id="xs-pipes-links"
                {{/compare}}>
                {{#each pipes}}
                    <li class="link">
                        <a href="{{relativeURL ../depth 'pipes'}}pipes/{{name}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if routes}}
        <li class="chapter">
            <a data-type="chapter-link" href="{{relativeURL depth 'routes'}}routes.html" {{#compare name "===" 'routes' }} class="active" {{/compare}}><span class="fa fa-code-fork"></span>Routes</a>
        </li>
        {{/if}}

        {{#if miscellaneous}}
        <li class="chapter">
            <a data-type="chapter-link" href="{{relativeURL depth 'miscellaneous'}}miscellaneous.html" {{#compare name "===" 'miscellaneous' }} class="active" {{/compare}}><span class="fa fa-cubes"></span>Miscellaneous</a>
        </li>
        {{/if}}

        {{#unless disableCoverage}}
        <li class="chapter">
            <a data-type="chapter-link" href="{{relativeURL depth 'coverage'}}coverage.html" {{#compare name "===" 'coverage' }} class="active" {{/compare}}><span class="fa fa-tasks"></span>Documentation coverage</a>
        </li>
        {{/unless}}

        {{#unless hideGenerator}}
        <li class="divider"></li>
        <li class="copyright">
                Documentation generated using <a href="https://compodoc.github.io/website/" target="_blank">
                    {{#if theme }}
                        {{#compare theme "indexof" 'readthedocs,vagrant,postman'}}
                            <img src="{{relativeURL depth 'logo'}}/images/compodoc-vectorise-inverted.svg" class="img-responsive">
                        {{else}}
                            <img src="{{relativeURL depth 'logo'}}/images/compodoc-vectorise.svg" class="img-responsive">
                        {{/compare}}
                    {{else}}
                    <img src="{{relativeURL depth 'logo'}}/images/compodoc-vectorise.svg" class="img-responsive">
                    {{/if}}
                </a>
        </li>
        {{/unless}}

    </ul>

</nav>
