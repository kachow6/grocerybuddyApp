<nav>

    <ul class="list">

        <li class="title">
            <a href="{{relativeURL depth pageType 'root'}}index.html">{{documentationMainName}}</a>
        </li>

        <li class="divider"></li>

        {{#compare menu "===" 'normal' }}
            {{> search-input }}
        {{/compare}}

        <li class="chapter">
            <a data-type="chapter-link" href="{{relativeURL depth pageType 'root'}}index.html"><span class="fa fa-home"></span>Getting started</a>
            <ul class="links">
                {{#if readme}}
                    <li class="link">
                        <a href="{{relativeURL depth pageType 'root'}}index.html" {{#compare name "===" 'index' }} class="active" {{/compare}}><span class="fa fa-file-text-o"></span>README</a>
                    </li>
                    <li class="link">
                        <a {{#compare context "===" 'readme'}} href="{{relativeURL depth pageType 'root'}}overview.html" {{/compare}}
                            {{#compare context "===" 'overview'}} href="{{relativeURL depth pageType 'root'}}overview.html" {{/compare}}
                            {{#compare context "!==" 'overview'}} href="{{relativeURL depth pageType 'root'}}overview.html" {{/compare}}
                            {{#compare context "!==" 'readme'}} href="{{relativeURL depth pageType 'root'}}overview.html" {{/compare}}
                            {{#compare name "===" 'overview' }} class="active" {{/compare}}>
                            <span class="fa fa-th"></span>Overview
                        </a>
                    </li>
                {{else}}
                    <li class="link">
                        <a href="{{relativeURL depth pageType 'root'}}" {{#compare name "===" 'overview' }} class="active" {{/compare}}>
                            <span class="fa fa-th"></span>Overview
                        </a>
                    </li>
                {{/if}}
            </ul>
        </li>

        {{#if additionalPages}}
        <li class="chapter">
            <div class="simple">
                <span class="fa fa-book"></span>
                <span>{{includesName}}</span>
                <span class="menu-toggler fa {{#isNotToggle 'additionalPages'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#additional-pages"
                    {{else}}
                        data-target="#xs-additional-pages"
                    {{/compare}}
                ></span>
            </div>
            <ul class="links collapse {{#isNotToggle 'additionalPages'}}in{{/isNotToggle}}"
                {{#compare menu "===" 'normal' }}
                    id="additional-pages"
                {{else}}
                    id="xs-additional-pages"
                {{/compare}}>
                {{#each additionalPages}}
                    <li class="link {{#compare depth "===" 2 }}for-chapter{{/compare}}">
                        <a href="{{relativeURL ../depth 'additional-page'}}{{path}}/{{filename}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if modules}}
        <li class="chapter">
            <a data-type="chapter-link" href="{{relativeURL depth pageType 'root'}}modules.html" {{#compare name "===" 'modules' }} class="active" {{/compare}}>
                <span class="fa fa-archive"></span>
                <span>Modules</span>
                <span class="menu-toggler fa {{#isNotToggle 'modules'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#modules-links"
                    {{else}}
                        data-target="#xs-modules-links"
                    {{/compare}}
                ></span>
            </a>
            <ul class="links collapse {{#isNotToggle 'modules'}}in{{/isNotToggle}}"
                {{#compare menu "===" 'normal' }}
                    id="modules-links"
                {{else}}
                    id="xs-modules-links"
                {{/compare}}>
                {{#each modules}}
                    <li class="link">
                        <a href="{{relativeURL ../depth 'modules'}}modules/{{name}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if components}}
        <li class="chapter">
            <div class="simple">
                <span class="fa fa-cogs"></span>
                <span>Components</span>
                <span class="menu-toggler fa {{#isNotToggle 'components'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#components-links"
                    {{else}}
                        data-target="#xs-components-links"
                    {{/compare}}></span>
            </div>
            <ul class="links collapse {{#isNotToggle 'components'}}in{{/isNotToggle}}"
                {{#compare menu "===" 'normal' }}
                    id="components-links"
                {{else}}
                    id="xs-components-links"
                {{/compare}}>
                {{#each components}}
                    <li class="link">
                        <a href="{{relativeURL ../depth 'components'}}components/{{name}}.html" {{#compare ../name "===" name }} data-type="entity-link" class="active" {{/compare}}>{{ name }}</a>
                    </li>
                {{/each}}
            </ul>
        </li>
        {{/if}}

        {{#if directives}}
        <li class="chapter">
            <div class="simple">
                <span class="fa fa-code"></span>
                <span>Directives</span>
                <span class="menu-toggler fa {{#isNotToggle 'directives'}}fa-angle-up{{else}}fa-angle-down{{/isNotToggle}}" data-toggle="collapse"
                    {{#compare menu "===" 'normal' }}
                        data-target="#directives-links"
                    {{else}}
